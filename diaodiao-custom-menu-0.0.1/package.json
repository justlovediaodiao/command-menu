{
	"name": "custom-menu",
	"displayName": "Custom Menu",
	"description": "Custome context menu to run command",
	"version": "0.0.1",
	"publisher": "diaodiao",
	"engines": {
		"vscode": "^1.23.1"
	},
	"activationEvents": [
		"onCommand:custom.customCommand"
	],
	"main": "./extension",
	"contributes": {
		"commands": [
			{
				"command": "custom.customCommand",
				"title": "custom command"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "custom.customCommand"
				}
			],
			"scm/resourceState/context": [
                {
                    "command": "custom.customCommand"
                }
			],
			"editor/context": [
                {
                    "command": "custom.customCommand"
                }
			]
		},
		"configuration": {
            "properties": {
                "custommenu.command": {
					"type": "string",
					"default": "",
                    "description": "command to run for custom menu"
                }
            }
        }
	}
}